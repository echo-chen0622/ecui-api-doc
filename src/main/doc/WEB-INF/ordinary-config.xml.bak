<?xml version="1.0" encoding="utf-8"?>
<ordinary-config version="0.1">
	<alias>
		<import-dom name="alias.xml" />
	</alias>
	<modules>
	</modules>

	<process>
		<host name=".*">
			<filter class="load" name="controls" file="controls.json" />
			<filter class="load" name="control" file="control.json" />
			<filter class="load" name="method" file="method.json" />
			<command name="/method/(.*)\.html">
				<result path="/method.html" />
			</command>
			<command name="/control/(.*)\.html">
				<result path="/control.html" />
			</command>
			<command name=".*\.html">
				<result />
			</command>
		</host>
	</process>
</ordinary-config>
